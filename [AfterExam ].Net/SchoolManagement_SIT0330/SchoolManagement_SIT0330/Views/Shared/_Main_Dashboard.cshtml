
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | Dashboard</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link href="~/Content/Theam/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />

    <!-- Ionicons -->

    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link href="~/Content/Theam/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- iCheck -->

    <link rel="stylesheet" href="~/Content/Theam/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->

    <link rel="stylesheet" href="~/Content/Theam/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->

    <link rel="stylesheet" href="~/Content/Theam/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->

    <link rel="stylesheet" href="~/Content/Theam/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->

    <link rel="stylesheet" href="~/Content/Theam/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->

    <link rel="stylesheet" href="~/Content/Theam/plugins/summernote/summernote-bs4.min.css">
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
        </div>
        @Html.Partial("_Navbar")

        <!-- Main Sidebar Container -->
        @Html.Partial("_Sidebar")




        @RenderBody()

        @Html.Partial("_Footer")
    </div>

    <!-- /.content-wrapper -->
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Content/Theam/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/Theam/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
  $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/Theam/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Content/Theam/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Content/Theam/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/Content/Theam/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Content/Theam/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/Theam/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Content/Theam/plugins/moment/moment.min.js"></script>
    <script src="plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Content/Theam/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/Content/Theam/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/Theam/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/Theam/dist/js/adminlte.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/Theam/dist/js/demo.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/Theam/dist/js/pages/dashboard.js"></script>
</body>
</html>

@{
    ViewBag.Title = "Main_Dashboard";
    Layout = "~/Views/Shared/Dashboard.cshtml";
